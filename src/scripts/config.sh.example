#!/usr/bin/env bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"
export DIR

# export PATH=<path to aws installation>:$PATH
export AWS_DEFAULT_REGION=us-east-1
export AWS_ACCOUNT_ID=<CHANGE-ME>
export AWS_ACCESS_KEY_ID=<CHANGE-ME>
export AWS_SECRET_ACCESS_KEY=<CHANGE-ME>
export AWS_EC2_SSH_KEYPAIR_PATH=../../awskeypair.pem
export AWS_SECURITY_GROUP=<CHANGE-ME>
export AWS_KEYPAIR_NAME=awskeypair
export AWS_REGION=$AWS_DEFAULT_REGION
